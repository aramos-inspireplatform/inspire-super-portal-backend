{
  "name": "inspire-super-portal-backend",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "NODE_ENV=development nest start --watch",
    "start:debug": "NODE_ENV=development nest start --debug 9230 --watch",
    "start:prod": "NODE_ENV=production node dist/main",
    "start:repl": "npm run start -- --entryFile repl",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "NODE_ENV=test jest --runInBand --detectOpenHandles --forceExit",
    "test:watch": "NODE_ENV=test jest --watch",
    "test:staged": "NODE_ENV=test jest --bail --findRelatedTests",
    "test:cov": "NODE_ENV=test jest --coverage",
    "test:debug": "NODE_ENV=test node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "NODE_ENV=test jest --config ./test/jest-e2e.json",
    "prepare": "husky install"
  },
  "dependencies": {
    "@fastify/compress": "^6.2.0",
    "@fastify/helmet": "^10.1.0",
    "@fastify/multipart": "^7.5.0",
    "@fastify/static": "^6.9.0",
    "@nestjs/axios": "^2.0.0",
    "@nestjs/common": "^9.0.0",
    "@nestjs/config": "^2.3.1",
    "@nestjs/core": "^9.0.0",
    "@nestjs/devtools-integration": "^0.1.3",
    "@nestjs/platform-fastify": "^9.3.12",
    "@nestjs/swagger": "^6.2.1",
    "@nestjs/typeorm": "^9.0.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "dotenv": "^16.0.3",
    "i": "^0.3.7",
    "newrelic": "^9.13.0",
    "npm": "^9.6.2",
    "pg": "^8.10.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.2.0",
    "typeorm": "^0.3.12"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@swc/core": "^1.3.42",
    "@swc/jest": "^0.2.24",
    "@types/jest": "29.2.4",
    "@types/newrelic": "^9.4.0",
    "@types/node": "18.11.18",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "git-commit-msg-linter": "^4.9.2",
    "husky": "^8.0.3",
    "jest": "29.3.1",
    "lint-staged": "^13.2.0",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "29.0.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.1.1",
    "typescript": "^4.7.4"
  },
  "engines": {
    "node": ">= 19.8.1",
    "npm": ">= 9.5.1"
  },
  "jest": {
    "coverageThreshold": {
      "global": {
        "lines": 90
      }
    },
    "moduleNameMapper": {
      "^~/(.*)$": "<rootDir>/src/$1",
      "^~test/(.*)+": "<rootDir>/test/$1"
    },
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "@swc/jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "<rootDir>/src/nestjs",
      "<rootDir>/src/common",
      "<rootDir>/src/main.ts",
      "index.ts"
    ],
    "roots": [
      "<rootDir>/test",
      "<rootDir>/src"
    ],
    "modulePaths": [
      "<rootDir>"
    ]
  }
}
