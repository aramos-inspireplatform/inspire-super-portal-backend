{
  "name": "Inspire Super Portal",
  "dockerComposeFile": [
    "docker-compose.yml"
  ],
  "service": "inspire_superportal_nestjs",
  "workspaceFolder": "/inspire_superportal_backend",
  "mounts": [
    "source=try-node-node_modules,target=${containerWorkspaceFolder}/node_modules,type=volume"
  ],
  "forwardPorts": [
    3336,
    9229,
    7001
  ],
  "customizations": {
    "vscode": {
      "extensions": [
        "dbaeumer.vscode-eslint",
        "editorconfig.editorconfig",
        "donjayamanne.githistory",
        "eamodio.gitlens",
        "gruntfuggly.todo-tree",
        "mhutchie.git-graph",
        "firsttris.vscode-jest-runner",
        "streetsidesoftware.code-spell-checker",
        "streetsidesoftware.code-spell-checker-portuguese",
        "github.copilot",
        "wallabyjs.wallaby-vscode"
      ],
      "settings": {
        "terminal.integrated.shell.linux": "/bin/zsh",
        "workbench.colorCustomizations": {
          "statusBar.background": "#6a329f",
          "statusBar.noFolderBackground": "#212121",
          "statusBar.debuggingBackground": "#263238"
        }
      }
    }
  }
}
}